 <section class="our_mission_Sec py_8">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-md-6">
                            <div class="our_mission_under">
                                <div class="text_our_miss">
                                    <h2>{{ section.settings.heading }}</h2>
                                    <p>{{ section.settings.title }}</p>
                                    <ul class="flex_ul_li">
                                       {% for block in section.blocks %}
                                        <li>
                                            <div class="ul_flex_txt">
                                                <p>{{ block.settings.bl-head }}</p>
                                            </div>
                                        </li>
                                      {% endfor %}
                                        
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="our_mission_image">
                               <img src="{{ section.settings.images | img_url: 'master' }}" alt>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
 
 {% schema %}
  {
    "name": "Our Mission",
    "settings": [
      {
      "type": "text",
      "id": "heading",
      "label": "Heading"
    },
      {
      "type": "textarea",
      "id": "title",
      "label": "Title"
    },
     
      {
      "type": "image_picker",
      "id": "images",
      "label": "Image"
    }
       
      
    ],
       "blocks": [
  {
    "type": "Our-Mission",
    "name": "Our Mission",
    "settings": [
     
      {
      "type": "textarea",
      "id": "bl-head",
      "label": "Add Text"
    }
      
    ]
  }
],
    
    
    "presets": [
      {
        "name": "Our Mission"
      }
    ]
    
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}